@article{POLLAK1978278,
title = {Some remarks on the Steiner problem},
journal = {Journal of Combinatorial Theory, Series A},
volume = {24},
number = {3},
pages = {278-295},
year = {1978},
issn = {0097-3165},
doi = {https://doi.org/10.1016/0097-3165(78)90058-4},
url = {https://www.sciencedirect.com/science/article/pii/0097316578900584},
author = {H.O Pollak},
abstract = {Let Σ be a set of n points in the plane. The minimal network for Σ is the tree of shortest total length LM(Σ) whose vertices are exactly the points of S. The Steiner minimal network for Σ is the tree of shortest possible total length LS(Σ) when the vertices are allowed to be any set Σ′ ⊇ Σ. Clearly LS(Σ) ⩽ LM(Σ), since the minimization in LS is over a larger set. It has long been conjectured that, conversely, LS(Σ) ⩾ (3122) LM(Σ), but this has previously been proved only if n = 3. In this paper, among other results, this is proved for n = 4. Unfortunately the proof is sufficiently complicated that immediate generalization to arbitrary n, no matter how desirable, is unlikely.}
}