author = {D Z Du  and F K Hwang },
title = {The Steiner ratio conjecture of Gilbert and Pollak is true.},
journal = {Proceedings of the National Academy of Sciences},
volume = {87},
number = {23},
pages = {9464-9466},
year = {1990},
doi = {10.1073/pnas.87.23.9464},
